  <link href="http://netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet">
    <style type="text/css">
        #header {
            color: black;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100px;
            padding: 0;
            margin: 0;
        }
		#header #background {
			background: white;
			filter:alpha(opacity=20); /* IE */
			-moz-opacity:0.2; /* Mozilla */
			opacity: 0.2; /* CSS3 */
			position: absolute;
			top: 0; 
			left: 0;
			height: 100%; 
			width:100%;
		}
        #header #content {
			position: absolute;
			top: 0; 
			left: 0;
			width: 100%; 
			height: 100%;
        }
		
		#header #content #nav button {
			margin: 3px;
		}

        #sign-up  
        {
            background: url(feet-image.jpg);
            background-size: 100%;
			background-repeat:no-repeat;
            height:800px;
        }
		
		#sign-up h1
		{
			margin:0px auto;
		}
        
    </style>

</head>

