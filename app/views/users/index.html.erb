<div class="content">
    <div id="title" class="text_center centered width80">
        <h1>Relevant People</h1>
    </div>
    <div id ="result" class="centered width80">
        <div class="centered width80">
            <% @users.each do |user| %>
                <span class="user_profile text-center" >
                    <%= default_picture(user) %>
                    <div class="name">
                        <%= user.name %>
                    </div>
                    <div class="position">
                        <%= user.program %>
                     </div>
                </span>
            <% end %>
        </div>
    </div>
<%= will_paginate (@users) %>
</div>

